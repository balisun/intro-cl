;;funcall, apply, reduce

(defvar *n* '(2 3 4 5))
(setf *n* '(2 3 4 5))
(* 2 3 4 5)
(funcall #'* 2 3 4 5)
(apply #'* *n*)
(apply #'* (remove-if #'oddp *n*))
(reduce #'list *n*)
(reduce #'list (remove-if #'oddp *n*))
(expt (expt (expt 2 3) 4) 5) ;((2^3)^4)^5
(expt 2 (* 3 4 5))
(reduce #'expt *n*)
(reduce #'list (remove-if #'oddp *n*))

(defvar *o* (list 1 (/ 1 3) (/ 1 4) (/ 1 5)))
(setf *o* (list 1 (/ 1 3) (/ 1 4) (/ 1 5)))
(append *n* *o*)
(reduce #'expt (append *n*
                       *o*))


